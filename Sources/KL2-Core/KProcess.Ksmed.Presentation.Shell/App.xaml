                <ResourceDictionary Source="/KProcess.Ksmed.Presentation.Core;component/JetPack/ToolkitStyles.xaml" />
                <ResourceDictionary Source="/KProcess.Ksmed.Presentation.Core;component/JetPack/CustomStyles.xaml" />

                <ResourceDictionary Source="/KProcess.Ksmed.Presentation.Core;component/Resources/MainResources.xaml" />
                <ResourceDictionary Source="/KProcess.Ksmed.Presentation.Core;component/Resources/SharedDataTemplates.xaml" />
                <ResourceDictionary Source="/KProcess.Ksmed.Presentation.Core;component/Resources/SharedControlTemplates.xaml" />
                <ResourceDictionary Source="/KProcess.Ksmed.Presentation.Core;component/Gantt/GanttResources.xaml" />
                <ResourceDictionary Source="/KProcess.Ksmed.Presentation.Core;component/Charting/ChartingResources.xaml" />
                <ResourceDictionary Source="/KL²;component/Resources/SharedResources.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
