            <Rectangle x:Name="overlay" Style="{TemplateBinding OverlayStyle}" />
            <Border x:Name="busycontent"  CornerRadius="4" Background="{TemplateBinding Background}" BorderBrush="{StaticResource DarkBorder}" 
