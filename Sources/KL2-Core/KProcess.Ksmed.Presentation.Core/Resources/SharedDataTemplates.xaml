                    </MultiBinding>
                </TextBlock.Visibility>
            </TextBlock>
            <TextBlock Text="{Binding Referential.Label}">
                <Hyperlink NavigateUri="{Binding Referential.CloudFile.Uri}">
                    <i:Interaction.Triggers>
                        <i:EventTrigger EventName="RequestNavigate">
