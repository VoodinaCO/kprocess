<UserControl x:Class="Kprocess.PackIconKprocess.PackIconCountriesFlags"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:converter="clr-namespace:Kprocess.PackIconKprocess.Converter"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             x:Name="myControl"
             d:DesignHeight="32"
             d:DesignWidth="32"
             mc:Ignorable="d">
    <Viewbox>
        <Image Source="{Binding Country, ElementName=myControl, Converter={converter:CountryToImageSourceConverter}}" />
    </Viewbox>
</UserControl>
