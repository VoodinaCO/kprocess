      </GeometryDrawing.Pen>
      <GeometryDrawing.Geometry>
        <PathGeometry FillRule="Nonzero" Figures="M30,0L30,30 M0,15L60,15" />
      </GeometryDrawing.Geometry>
    </GeometryDrawing>
    <GeometryDrawing Brush="#FF000000">
      <GeometryDrawing.Pen>
        <Pen Brush="#FFCF142B" Thickness="6" StartLineCap="Flat" EndLineCap="Flat" LineJoin="Miter" />
      </GeometryDrawing.Pen>
      <GeometryDrawing.Geometry>
        <PathGeometry FillRule="Nonzero" Figures="M30,0L30,30 M0,15L60,15" />
      </GeometryDrawing.Geometry>
    </GeometryDrawing>
