      <GeometryDrawing.Geometry>
        <PathGeometry FillRule="Nonzero" Figures="M0,0L60,30 M60,0L0,30" />
      </GeometryDrawing.Geometry>
    </GeometryDrawing>
    <DrawingGroup>
